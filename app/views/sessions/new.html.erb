<h1>Login</h1>

<% provide(:title, "Login") %>

<%= form_for(:session, url: login_path) do |form| %>

    <%= form.label :email %>
    <%= form.email_field :email %>

    <br>
    <br>

    <%= form.label :password %>
    <%= form.password_field :password %>

    <br>
    <br>
  
    <%= form.submit "Log in" %>
<% end %>

<p><%= link_to "Sign up now!", new_user_path %></p>
